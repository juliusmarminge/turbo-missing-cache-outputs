{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./bundler.json",
  "compilerOptions": {
    /** Emit types for internal packages to speed up editor performance. */
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "outDir": "${configDir}/dist",
    "module": "NodeNext",
    "moduleResolution": "NodeNext"
  }
}
